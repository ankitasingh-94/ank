{
  "name": "rhinomatic",
  "version": "1.0.0",
  "description": "Automated tools for testing.",
  "main": "index.js",
  "directories": {
    "test": "src/test"
  },
  "scripts": {
    "test": "mkdir -p results && jest",
    "test-canary": "mkdir -p results && jest src/test/newOrgCanary --detectOpenHandles",
    "test-existing-canary": "mkdir -p results && jest src/test/existingOrgCanary",
    "test-integrations": "mkdir -p results && jest src/test/integrations",
    "clean": "rm -rf lib/* results",
    "build": "yarn clean && node_modules/.bin/babel src --out-dir lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhinogram/rhinomatic.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rhinogram/rhinomatic/issues"
  },
  "homepage": "https://github.com/rhinogram/rhinomatic#readme",
  "devDependencies": {
    "@babel/cli": "7.4.3",
    "@babel/core": "7.4.3",
    "@babel/node": "7.2.2",
    "@babel/preset-env": "7.4.3",
    "eslint": "5.16.0",
    "rhinodeploy-cli": "git+https://ab1e60bc7d1ea8f0cef2d98a042749dee2741f8c:x-oauth-basic@github.com/rhinogram/rhinodeploy.git#cli"
  },
  "dependencies": {
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "browserstack-automate": "1.0.2",
    "chromedriver": "2.46.0",
    "dotenv": "7.0.0",
    "express": "4.16.4",
    "geckodriver": "1.16.2",
    "imap": "0.8.19",
    "jest-html-reporter": "2.5.0",
    "jest-junit": "6.3.0",
    "jest": "24.7.1",
    "moment-timezone": "0.5.23",
    "nightwatch-api": "2.1.3",
    "nightwatch": "1.0.19",
    "request": "2.88.0",
    "rhinocloud-sdk": "1.0.xx",
    "rhinotilities": "git+https://18c4f4a191d4d4b355f3b7c03814a85c8e514b78@github.com/rhinogram/rhinotilities#1.0.18",
    "selenium-server-standalone-jar": "3.141.5",
