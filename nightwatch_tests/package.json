{
  "name": "nightwatch",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "nightwatch --skipgroup deprecated",
    "canary": "nightwatch --group canary || echo \"Some test(s) have failed. Check the slack channel, nightwatch_reporter\"",
    "clean": "rm -rf lib/*",
    "build": "yarn clean && yarn && node_modules/.bin/babel tests --out-dir lib/tests && node_modules/.bin/babel page_objects --out-dir lib/page_objects && node_modules/.bin/babel helpers.js --out-dir lib/ && node_modules/.bin/babel globals.js --out-dir lib/"
  },
  "author": "Rhinogram, LLC",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-cli": "6.26.x",
    "babel-preset-env": "1.7.x",
    "eslint": "5.1.x"
  },
  "dependencies": {
    "browserstack-automate": "1.0.2",
    "chromedriver": "2.38.x",
    "dotenv": "6.0.x",
    "moment-timezone": "0.5.23",
    "nightwatch": "0.9.x",
    "nightwatch-html-reporter": "2.0.5",
    "nightwatch-slack-reporter": "0.0.2",
    "selenium-server-standalone-jar": "3.8.x",
    "testarmada-magellan": "10.0.x",
    "testarmada-magellan-local-executor": "2.0.x",
    "testarmada-magellan-nightwatch-plugin": "7.0.x",
    "testarmada-nightwatch-extra": "4.1.x"
  }
}
